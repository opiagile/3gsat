<?xml version="1.0" encoding="utf-8"?> 
<mx:Canvas 
	xmlns:ns1="com.google.maps.controls.*" width="366"
	creationComplete="creationComplete();">
<maps:Map xmlns:maps="com.google.maps.*" id="map" mapevent_mapready="onMapReady(event)" width="366" height="530" key="your_api_key" x="0" y="10"/> 
<mx:Script> 
    <![CDATA[ 
    
    import com.google.maps.LatLng; 
    import com.google.maps.Map; 
    import com.google.maps.MapEvent; 
    import com.google.maps.MapType; 
 
    private function onMapReady(event:Event):void { 
      this.map.setCenter(new LatLng(-25.366085,-49.220698), 14, MapType.NORMAL_MAP_TYPE); 
    } 
    ]]> 
</mx:Script> 
	<ns1:ControlBase x="612" y="133"/>
</mx:Canvas>